<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
    <servlet>
        <servlet-name>ChannelConnecting</servlet-name>
        <servlet-class>org.spine3.client.grpc.web.ChannelConnectingWebImpl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ChannelConnecting</servlet-name>
        <url-pattern>/channelConnecting</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ClientService</servlet-name>
        <servlet-class>org.spine3.client.grpc.web.ClientServiceWebImpl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ClientService</servlet-name>
        <url-pattern>/clientService</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Dispatcher</servlet-name>
        <servlet-class>org.spine3.client.grpc.web.dispatcher.DispatcherServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Dispatcher</servlet-name>
        <url-pattern>/dispatcher</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>